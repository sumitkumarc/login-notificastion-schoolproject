                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                card_view:cardElevation="5dp"
                card_view:cardMaxElevation="10dp">

                <LinearLayout
